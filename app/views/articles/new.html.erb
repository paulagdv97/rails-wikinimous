<div class = "container">
  <h1>Create new article ✍🏽</h1>
  <%= form_for(@article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

  <p><%=link_to "Back to all articles!", articles_path %></p>
</div>
